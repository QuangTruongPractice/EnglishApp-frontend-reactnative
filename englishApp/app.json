{
  "expo": {
    "name": "EnglishApp",
    "slug": "englishApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-image.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-image.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.tranquangtruong25.englishApp"
    },
    "web": {
      "favicon": "./assets/logo-image.png"
    },
    "plugins": [
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "eas": {
        "projectId": "7a3b441d-941b-46f9-9de6-0ae859c13bd9"
      }
    }
  }
}